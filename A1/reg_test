cd /u/cos333/Asgt1Solution

./reg > /u/dbasili/r1
./reg -dept COS > /u/dbasili/r2
./reg -coursenum 333 > /u/dbasili/r3
./reg -coursenum b > /u/dbasili/r4
./reg -area Qr > /u/dbasili/r5
./reg -title intro > /u/dbasili/r6
./reg -title science > /u/dbasili/r7
./reg -title C_S > /u/dbasili/r8
./reg -title c%S > /u/dbasili/r9
./reg -dept cos -coursenum 3 > /u/dbasili/r10
./reg -dept cos -area qr -coursenum 2 -title intro > /u/dbasili/r11
./reg -title "Independent Study" > /u/dbasili/r12
./reg -title "Independent Study " > /u/dbasili/r13
./reg -title "Independent Study  " > /u/dbasili/r14
./reg -title " Independent Study" > /u/dbasili/r15
./reg -title "  Independent Study" > /u/dbasili/r16

./reg -area qr st 2> /u/dbasili/r17
./reg area qr 2> /u/dbasili/r18
./reg -Area qr 2> /u/dbasili/r19
./reg "-area " qr 2> /u/dbasili/r20
./reg -h -area qr -h 2> /u/dbasili/r21
./reg -dept cos -coursenum 333 -dept cos 2> /u/dbasili/r22
./reg -area 2> /u/dbasili/r23
./reg -h -area qr -dept 2> /u/dbasili/r24

cd /u/dbasili

./reg > /u/dbasili/rr1
./reg -dept COS > /u/dbasili/rr2
./reg -coursenum 333 > /u/dbasili/rr3
./reg -coursenum b > /u/dbasili/rr4
./reg -area Qr > /u/dbasili/rr5
./reg -title intro > /u/dbasili/rr6
./reg -title science > /u/dbasili/rr7
./reg -title C_S > /u/dbasili/rr8
./reg -title c%S > /u/dbasili/rr9
./reg -dept cos -coursenum 3 > /u/dbasili/rr10
./reg -dept cos -area qr -coursenum 2 -title intro > /u/dbasili/rr11
./reg -title "Independent Study" > /u/dbasili/rr12
./reg -title "Independent Study " > /u/dbasili/rr13
./reg -title "Independent Study  " > /u/dbasili/rr14
./reg -title " Independent Study" > /u/dbasili/rr15
./reg -title "  Independent Study" > /u/dbasili/rr16

./reg -area qr st 2> /u/dbasili/rr17
./reg area qr 2> /u/dbasili/rr18
./reg -Area qr 2> /u/dbasili/rr19
./reg "-area " qr 2> /u/dbasili/rr20
./reg -h -area qr -h 2> /u/dbasili/rr21
./reg -dept cos -coursenum 333 -dept cos 2> /u/dbasili/rr22
./reg -area 2> /u/dbasili/rr23
./reg -h -area qr -dept 2> /u/dbasili/rr24

echo diff 1
diff r1 rr1

echo diff 2
diff r2 rr2

echo diff 3
diff r3 rr3

echo diff 4
diff r4 rr4

